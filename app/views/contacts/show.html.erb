<div class="mt-4 text-5xl font-semibold border-b-2 ">Consultation History</div>
<div class="mt-4 flex flex-wrap">
  <%= render 'consultations/create_form' %>
  <% @contact.consultations.where.not(id: nil).order(:created_at).reverse.map do |consultation| %>
    <div class="md:w-1/3 w-full p-2">
      <div class="bg-white shadow overflow-hidden  sm:rounded-lg mt-4 p-4 h-full">
        <div class="mt-4 text-3xl font-semibold px-2 -py-1 rounded shadow bg-<%= consultation.action.color %>-200">
          <%= consultation&.action&.name&.titleize %>
        </div>
        <div class="mb-2">
          Requested <%= time_ago_in_words(consultation.created_at) %> ago
        </div>
        <div class="mb-2 text-xs">
          (<%= consultation.created_at.strftime("%m/%d/%Y at %I:%M%p") %>)
        </div>
        <div class="mt-8 sm:mt-0 sm:grid sm:grid-cols-3 sm:gap-4 sm:border-t sm:border-gray-200 sm:py-5">
          <dt class="text-sm leading-5 font-medium text-gray-500">
            Consultation type
          </dt>
          <dd class="mt-1 text-sm leading-5 text-gray-900 sm:mt-0 sm:col-span-2">
            <%= consultation.consultation_type %>
          </dd>
        </div>
        <div class="mt-8 sm:mt-0 sm:grid sm:grid-cols-3 sm:gap-4 sm:border-t sm:border-gray-200 sm:py-5">
          <dt class="text-sm leading-5 font-medium text-gray-500">
            Source of Call
          </dt>
          <dd class="mt-1 text-sm leading-5 text-gray-900 sm:mt-0 sm:col-span-2">
            <%= consultation.source %>
          </dd>
        </div>
        <% if consultation.doctor_id.blank? && current_user.is_admin? %>
          <%= render 'consultations/assign_form', consultation: consultation %>
        <% else %>
          <div class="mt-8 sm:mt-0 sm:grid sm:grid-cols-3 sm:gap-4 sm:border-t sm:border-gray-200 sm:py-5">
            <dt class="text-sm leading-5 font-medium text-gray-500">
              Assigned to
            </dt>
            <dd class="mt-1 text-sm leading-5 text-gray-900 sm:mt-0 sm:col-span-2">
              <%= consultation.doctor&.name %>
            </dd>
          </div>
          <% if current_user.is_admin? %>
            <div>
              <%= link_to 'View Details', consultation_path(consultation), class: 'hover:text-white hover:bg-gray-600 inline-flex items-center px-3 py-2 rounded-md text-sm font-medium leading-5 bg-gray-100 text-gray-800' %>
            </div>
          <% end %>
        <% end %>
      </div>
    </div>
  <% end %>
</div>
<div class="mt-4 text-5xl font-semibold border-b-2 ">Prescriptions</div>
<div class="mt-4 flex flex-wrap">
  <% @contact.prescriptions.where.not(id: nil).order(:created_at).reverse.map do |prescription| %>
    <div class="md:w-1/3 w-full p-2">
      <div class="bg-white shadow overflow-hidden  sm:rounded-lg mt-4 p-4 h-full">
        <div class="mt-4 text-3xl font-semibold px-2 -py-1 rounded shadow bg-gray-200">
          <%= prescription.delivery_status.titleize %>
        </div>
        <div class="mb-2">
          Prescribed <%= time_ago_in_words(prescription.created_at) %> ago
        </div>
        <div class="mt-8 sm:mt-0 sm:grid sm:grid-cols-3 sm:gap-4 sm:border-t sm:border-gray-200 sm:py-5">
          <dt class="text-sm leading-5 font-medium text-gray-500">
            Source
          </dt>
          <dd class="mt-1 text-sm leading-5 text-gray-900 sm:mt-0 sm:col-span-2">
            <%= prescription.source %>
          </dd>
        </div>
        <div class="mt-8 sm:mt-0 sm:grid sm:grid-cols-3 sm:gap-4 sm:border-t sm:border-gray-200 sm:py-5">
          <dt class="text-sm leading-5 font-medium text-gray-500">
            Scheme
          </dt>
          <dd class="mt-1 text-sm leading-5 text-gray-900 sm:mt-0 sm:col-span-2">
            <%= prescription.scheme %>
          </dd>
        </div>
        <div class="mt-8 sm:mt-0 sm:grid sm:grid-cols-3 sm:gap-4 sm:border-t sm:border-gray-200 sm:py-5">
          <dt class="text-sm leading-5 font-medium text-gray-500">
            Supplier
          </dt>
          <dd class="mt-1 text-sm leading-5 text-gray-900 sm:mt-0 sm:col-span-2">
            <%= prescription.supplier %>
          </dd>
        </div>
      </div>
    </div>
  <% end %>

</div>
